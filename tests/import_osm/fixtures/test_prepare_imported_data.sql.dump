CREATE VIEW osm_aggregated_merged_linestrings_view AS
 SELECT min(osm_linestring.id) AS id,
    max(((osm_linestring.osm_id)::character varying)::text) AS osm_id,
    min(osm_linestring.parent_id) AS parent_id,
    max((osm_linestring.type)::text) AS type,
    st_union(array_agg(osm_linestring.geometry)) AS geometry,
    max(osm_linestring.place_rank) AS place_rank,
    max((osm_linestring.name)::text) AS name,
    max((osm_linestring.name_fr)::text) AS name_fr,
    max((osm_linestring.name_en)::text) AS name_en,
    max((osm_linestring.name_de)::text) AS name_de,
    max((osm_linestring.name_es)::text) AS name_es,
    max((osm_linestring.name_ru)::text) AS name_ru,
    max((osm_linestring.name_zh)::text) AS name_zh,
    min((osm_linestring.country_code)::text) AS country_code,
    max((osm_linestring.wikidata)::text) AS wikidata,
    max((osm_linestring.wikipedia)::text) AS wikipedia
   FROM osm_linestring
  WHERE (osm_linestring.merged_into IS NOT NULL)
  GROUP BY osm_linestring.merged_into;


